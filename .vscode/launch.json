    "version": "0.2.0",
    "configurations": [
        {
            "name": "PowerShell Launch Current File",
            "type": "PowerShell",
            "request": "launch",
            "script": "${file}",
            "cwd": "${file}"
        },
        {
            "type": "msedge",
            "request": "launch",
            "name": "Iniciar Edge para localhost",
            "url": "https://localhost:9999",
            "webRoot": "${workspaceFolder}"
        }
    ]
}