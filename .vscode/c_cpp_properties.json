{
    "configurations": [
        {
            "name": "InsaneEmscripten-Debug",
            "includePath": [
                "${workspaceFolder}",
                "${workspaceFolder}/Include",
                "${workspaceFolder}/modules/InsaneCpp/Include",
                "${workspaceFolder}/modules/CommonCppIncludes",
                "${workspaceFolder}/modules/Botan-2.19.3-Emscripten-Llvm-Release/include/botan-2",
                "${EMSCRIPTEN_SDK}/upstream/emscripten/system/include",
                "${EMSCRIPTEN_SDK}/upstream/emscripten/cache/sysroot/include"
            ],
            "compilerArgs": [
                "-I ${EMSCRIPTEN_SDK}/upstream/emscripten/cache/sysroot/include",
                "-I ${EMSCRIPTEN_SDK}/upstream/emscripten/system/include"
            ],
            "defines": [
                "_DEBUG",
                "UNICODE",
                "_UNICODE",
                "INSANE_EXPORTS"
            ],
            "compilerPath": "${EMSCRIPTEN_COMPILER}",
            "cStandard": "c17",
            "cppStandard": "c++20",
            "intelliSenseMode": "clang-x64",
            "configurationProvider": "ms-vscode.makefile-tools"
        },

        {
            "name": "InsaneEmscripten-Release",
            "includePath": [
                "${workspaceFolder}",
                "${workspaceFolder}/Include",
                "${workspaceFolder}/modules/InsaneCpp/Include",
                "${workspaceFolder}/modules/CommonCppIncludes",
                "${workspaceFolder}/modules/Botan-2.19.3-Emscripten-Llvm-Release/include/botan-2",
                "${EMSCRIPTEN_SDK}/upstream/emscripten/system/include",
                "${EMSCRIPTEN_SDK}/upstream/emscripten/cache/sysroot/include"
            ],
            "defines": [
                "NDEBUG",
                "UNICODE",
                "_UNICODE",
                "INSANE_EXPORTS"
            ],
            "compilerPath": "${EMSCRIPTEN_COMPILER}",
            "cStandard": "c17",
            "cppStandard": "c++20",
            "intelliSenseMode": "clang-x64",
            "configurationProvider": "ms-vscode.makefile-tools"
        }
    ],
    "version": 4
}