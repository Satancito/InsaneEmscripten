{
    "configurations": [
        {
            "name": "InsaneEmscripten",
            "includePath": [
                "${workspaceFolder}",
                "${workspaceFolder}/include",
                "${workspaceFolder}/../InsaneCpp/include",
                "${EMSCRIPTEN_SDK}/upstream/emscripten/system/include",
                "${EMSCRIPTEN_SDK}/upstream/emscripten/cache/sysroot/include"
            ],
            "defines": [
                "_DEBUG",
                "UNICODE",
                "_UNICODE",
                "__EMSCRIPTEN__"
            ],
            "compilerPath": "${EMSCRIPTEN_SDK}/upstream/emscripten/em++",
            "cStandard": "c11",
            "cppStandard": "c++17",
            "intelliSenseMode": "linux-clang-x64"
        }
    ],
    "version": 4
}