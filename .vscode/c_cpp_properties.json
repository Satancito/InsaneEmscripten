{
    "configurations": [
        {
            "name": "InsaneEmscripten",
            "includePath": [
                "${workspaceFolder}",
                "${workspaceFolder}/include",
                "${workspaceFolder}/../InsaneCpp/include",
                "${EMSCRIPTEN_ROOT}/upstream/emscripten/system/include",
                "${EMSCRIPTEN_ROOT}/upstream/emscripten/cache/sysroot/include",
                "${workspaceFolder}/../CommonCppLibs/Botan-2.19.3-Emscripten-Llvm-Release/include/botan-2"
            ],
            "defines": [
                "_DEBUG",
                "UNICODE",
                "_UNICODE",
                "__EMSCRIPTEN__"
            ],
            "compilerPath": "${EMSCRIPTEN_COMPILER}",
            "cStandard": "c11",
            "cppStandard": "c++20",
            "intelliSenseMode": "linux-clang-x64",
            "configurationProvider": "ms-vscode.makefile-tools"
        }
    ],
    "version": 4
}