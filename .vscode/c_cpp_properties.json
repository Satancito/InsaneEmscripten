{
    "configurations": [
        {
            "name": "InsaneEmscripten",
            "includePath": [
                "${workspaceFolder}",
                "${workspaceFolder}/include",
                "${workspaceFolder}/../InsaneCpp/include",
                "${EMSCRIPTEN_DIR}/upstream/emscripten/system/include",
                "${EMSCRIPTEN_DIR}/upstream/emscripten/cache/sysroot/include"
            ],
            "defines": [
                "_DEBUG",
